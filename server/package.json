{
  "name": "gifcipes-server",
  "version": "0.1.0",
  "description": "Gifcipes server",
  "main": "index.js",
  "scripts": {
    "start": "node lib/index.js",
    "debug": "node index.js",
    "db:start": "docker run -d -p 28015:28015 -p :8090:8080 -v $PWD/db:/data --name gifcipesdb rethinkdb",
    "db:stop": "docker stop gifcipesdb",
    "db:rm": "docker rm gifcipesdb",
    "test": "eslint src/ && NODE_ENV=testing node test/index.js | tap-spec",
    "postinstall": "babel src --out-dir lib"
  },
  "keywords": [
    "javascript",
    "recipes"
  ],
  "author": "Aleksandar Gadjev",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gadzev/gifcipes.git"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "body-parser": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "crypto": "0.0.3",
    "express": "^4.14.0",
    "express-session": "^1.15.0",
    "jsonwebtoken": "^7.2.1",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "thinky": "^2.3.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "reqlite": "^2.3.0",
    "supertest": "^3.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "babel": {
    "presets": [
      "es2015-node",
      "stage-0"
    ]
  }
}
